{
    "name": "potherca/phpunit-mock-function",
    "description": "Trait that allows mocking functions in PhpUnit testcase.",
    "type": "library",
    "license": "GPL-3.0",
    "require": {
        "phpunit/php-text-template": "^1.0",
        "phpunit/phpunit": "^5.0|^6.0"
    },
    "autoload": {
        "files": [
            "src/requireOnce.php"
        ],
        "psr-4": {
            "Potherca\\Phpunit\\MockFunction\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Potherca\\Phpunit\\MockFunction\\": "tests/"
        }
    }
}
